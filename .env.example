# Notion integration token (from notion.so/my-integrations)
NOTION_API_KEY=ntn_your_notion_integration_token

# Notion database ID (the 32-char hex ID from your database URL)
NOTION_DB_ID=your_notion_database_id

# Anthropic API key (for Claude Haiku question extraction)
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key

# OpenAI API key (for GPT web-search research)
OPENAI_API_KEY=sk-proj-your_openai_key

# Gmail credentials for digest emails (daemon mode only)
GMAIL_USER=you@gmail.com
GMAIL_APP_PASSWORD=your_google_app_password
